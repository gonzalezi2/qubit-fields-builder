{
  "private": true,
  "name": "fields-builder",
  "version": "1.1",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "parcel ./src/index.html -d docs -p 8080 --open",
    "build": "rm -rf docs && parcel build src/index.html -d docs --public-url ./ --no-source-maps",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor",
    "rules": {
      "indent": [
        "error",
        2
      ],
      "react/jsx-indent-props": [
        "enabled",
        2
      ]
    }
  },
  "eslintIgnore": [
    "docs/*"
  ],
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node-sass": "^4.11.1",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-config-synacor": "^2.0.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^25.1.0",
    "jest-cli": "^24.9.0",
    "jest-localstorage-mock": "^2.4.0",
    "node-sass": "^4.13.1",
    "parcel-bundler": "^1.12.4",
    "preact-cli": "^2.2.1",
    "preact-render-spy": "^1.3.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "linkstate": "^1.1.1",
    "preact": "^10.4.4",
    "preact-render-to-string": "^5.0.6",
    "preact-router": "^3.0.1",
    "prismjs": "^1.17.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "src": "^1.1.2",
    "uuid": "^8.0.0"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat",
    "react-dom/test-utils": "preact/test-utils"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "verbose": true,
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>setupTests.js"
    ],
    "testRegex": "(/(__tests__|tests)/.*|(\\.|/)(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "js",
      "ts",
      "jsx",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "^./style$": "identity-obj-proxy",
      "^preact$": "<rootDir>/node_modules/preact/dist/preact.umd.js",
      "^react$": "preact/compat",
      "^react-dom$": "preact/compat",
      "^react-dom/test-utils$": "preact-test-utils",
      "^create-react-class$": "preact/compat/lib/create-react-class",
      "^react-addons-css-transition-group$": "preact-css-transition-group"
    }
  }
}
